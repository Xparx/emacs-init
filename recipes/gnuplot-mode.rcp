(:name gnuplot-mode
       :description "Drive gnuplot from within emacs"
       :type github
       :pkgname "bruceravel/gnuplot-mode"
       :build `(("aclocal && autoconf")
                ("./configure")
                ("automake --add-missing")
                ("make" ,(concat "EMACS=" el-get-emacs)
                 "gnuplot.elc" "gnuplot-gui.elc"))
       :info "gnuplot.info")
